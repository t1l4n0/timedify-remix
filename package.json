{
  "name": "timedify-remix",
  "private": true,
  "type": "module",
  "main": "start-server.js",
  "module": "build/server/index.js",
  "types": "build/server/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./build/server/index.js",
      "require": "./build/server/index.cjs",
      "default": "./build/server/index.cjs"
    }
  },
  "workspaces": {
    "packages": [
      "extensions/*"
    ]
  },
  "scripts": {
    "build": "remix build && npm run build:server",
    "build:client": "remix build",
    "build:server": "vite build --config vite.server.enhanced.config.ts",
    "config:link": "shopify app config link",
    "config:use": "shopify app config use",
    "deploy": "shopify app deploy",
    "dev": "shopify app dev",
    "docker-start": "npm run setup && node start-server.js",
    "env": "shopify app env",
    "generate": "shopify app generate",
    "graphql-codegen": "graphql-codegen",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "prisma generate && node scripts/patch-shopify-imports.js",
    "patch:shopify": "node scripts/patch-shopify-imports.js",
    "prisma": "prisma",
    "setup": "prisma generate && prisma migrate deploy && npm run patch:shopify",
    "shopify": "shopify",
    "start": "NODE_ENV=production node start-server.js",
    "start:dev": "NODE_ENV=development node start-server.js",
    "start:original": "remix-serve ./build/server/index.js",
    "vite": "vite"
  },
  "resolutions": {
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "@graphql-tools/url-loader": "8.0.16",
    "minimatch": "9.0.5"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@remix-run/dev": "^2.16.1",
    "@remix-run/fs-routes": "^2.16.1",
    "@remix-run/node": "^2.16.1",
    "@remix-run/react": "^2.16.1",
    "@remix-run/serve": "^2.16.1",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/polaris": "^12.0.0",
    "@shopify/shopify-app-remix": "^3.7.0",
    "@types/compression": "^1.8.1",
    "compression": "^1.8.0",
    "crypto-browserify": "^3.12.1",
    "https-browserify": "^1.0.0",
    "isbot": "^5.1.0",
    "prisma": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@remix-run/eslint-config": "^2.16.1",
    "@remix-run/route-config": "^2.16.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@shopify/shopify-app-express": "^5.0.18",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.7",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.2.4",
    "rollup": "^4.43.0",
    "terser": "^5.43.0",
    "typescript": "^5.2.2",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": "^18.20 || ^20.10 || >=21.0.0"
  },
  "overrides": {
    "@graphql-tools/url-loader": "8.0.16",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "minimatch": "9.0.5"
  },
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ]
}
